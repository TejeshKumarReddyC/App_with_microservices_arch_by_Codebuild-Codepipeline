[
  {
    "name": "frontend",
    "imageUri": "<IMAGE_URI>",
    "portMappings": [
      {
        "containerPort": 80,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "environment": [
      {
        "name": "BACKEND_URL",
        "value": "http://backend.myapp.local:5000"
      }
    ]
  }
]
